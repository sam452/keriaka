  Latest prices for <%= @commodity.name %>
  <table>
<% @prices.each do |p| %>
  <tr>
    <td>Price: <%= p.price %></td>
    <td>Quality: <%= p.quality %></td>
    <td>Buyer: <%= p.buyer %></td>
    <td>Date: <%= p.date.try(:strftime,'%m/%d/%Y') %></td>
  </tr>
<% end %>
  </table>
<%= link_to "New Price Report", new_commodity_price_path(@commodity) %>